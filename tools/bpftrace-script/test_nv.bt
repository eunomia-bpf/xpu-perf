#!/usr/bin/env bpftrace
/*
 * Test ALL nv_* functions (monolithic version)
 * WARNING: This attaches to ~530 probes - may fail or be slow
 */

BEGIN {
    printf("Tracing ALL nv_* functions...\n");
}

kprobe:nv_*
{
    @calls[probe] = count();
}

END {
    printf("\n=== ALL nv_* Function Calls ===\n");
    print(@calls);
    clear(@calls);
}
