#!/usr/bin/env bpftrace
/*
 * Test ALL uvm_* functions (monolithic version)
 * WARNING: This attaches to ~1300 probes - may fail or be slow
 */

BEGIN {
    printf("Tracing ALL uvm_* functions...\n");
}

kprobe:uvm_*
{
    @calls[probe] = count();
}

END {
    printf("\n=== ALL uvm_* Function Calls ===\n");
    print(@calls);
    clear(@calls);
}
